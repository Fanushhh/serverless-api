{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",         
        "use": "@vercel/node"          
      }
    ],
    "routes": [
        {
            "src": "/api/v1/tasks",  
            "dest": "/api/index.js"     
          },
      {
        "src": "/api/v1/tasks/(.*)",   
        "dest": "/api/index.js"        
      }
    ]
  }
  